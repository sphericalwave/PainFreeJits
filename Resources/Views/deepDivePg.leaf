                                <input type="number" class="form-control" id="exampleInputPassword1" placeholder="23">
                                    <br>
                                    
                                <div class="mybox2">
                                    <input type="radio" id="white" name="belt" value="white" required>
                                    <label for="white">Male</label>

                                    <input type="radio" id="white" name="belt" value="white" required>
                                    <label for="white">Female</label>
                                </div>
                                
                                <label for="white">Email</label><br>
                                                                <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="g_ryan@ds.com">
                                
                                </form>
                            </div> <!--mybox-->
